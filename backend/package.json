{
  "name": "kaushalx-backend",
  "version": "1.0.0",
  "description": "AI-powered course trend analysis backend",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "ai-discover": "node -e \"require('./services/aiWebSearcher').searchAndAnalyzeCourses().then(r => console.log(r))\"",
    "startup": "node scripts/startup.js",
    "init-ai": "node scripts/initializeAI.js",
    "test-ai": "node scripts/testAI.js",
    "test-all-ai": "node scripts/testAllAI.js",
    "quick-ai-test": "node scripts/quickAITest.js",
    "fix-analysis": "node scripts/fixCourseAnalysis.js",
    "comprehensive-fix": "node scripts/comprehensiveFix.js",
    "add-trending": "node scripts/addTrendingCourses.js",
    "debug-courses": "node scripts/debugCourses.js",
    "fix-trends": "node scripts/fixCourseTrends.js",
    "test-ai-status": "node scripts/testAIStatus.js",
    "check-db": "node scripts/checkDatabase.js",
    "test-endpoints": "node scripts/testEndpoints.js",
    "check-trends": "node scripts/fixTrendValues.js",
    "system-check": "node scripts/completeSystemCheck.js",
    "setup": "npm install && npm run startup && npm run add-trending",
    "test-fixes": "node test-fixes.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "mongoose": "^7.5.0",
    "axios": "^1.5.0",
    "node-cron": "^3.0.2",
    "ws": "^8.13.0",
    "helmet": "^7.0.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "natural": "^6.5.0",
    "cheerio": "^1.0.0-rc.12"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.6.2"
  }
}